PROJECT_NAME           = "SIDERA"
PROJECT_BRIEF          = by mrsinho.
PROJECT_NUMBER         = ${SIDERA_VERSION}
PROJECT_LOGO           = 

USE_MDFILE_AS_MAINPAGE = ${SIDERA_REPO_ROOT_DIR}/README.md

OUTPUT_DIRECTORY       = docs
INPUT                  = ${SIDERA_REPO_ROOT_DIR}/sidera/include ${SIDERA_REPO_ROOT_DIR}/README.md ${SIDERA_REPO_ROOT_DIR}/docs/src/application-tutorial.md ${SIDERA_REPO_ROOT_DIR}/docs/src/quick-install.md ${SIDERA_REPO_ROOT_DIR}/docs/src/terminal-install.md ${SIDERA_REPO_ROOT_DIR}/.shci/linux/log.md ${SIDERA_REPO_ROOT_DIR}/.shci/windows/log.md 
RECURSIVE              = YES
EXTRACT_ALL            = YES
HAVE_DOT               = NO
HIDE_UNDOC_MEMBERS     = YES
GENERATE_HTML          = YES
GENERATE_XML           = YES
GENERATE_LATEX         = YES
USE_MATHJAX            = YES
GENERATE_TREEVIEW      = YES
DISABLE_INDEX          = NO
FULL_SIDEBAR           = NO
OPTIMIZE_OUTPUT_FOR_C  = YES
STRIP_CODE_COMMENTS    = NO

IMAGE_PATH             = ${SIDERA_REPO_ROOT_DIR}/docs/media ${SIDERA_REPO_ROOT_DIR}/.shci/linux ${SIDERA_REPO_ROOT_DIR}/.shci/windows

HTML_COLORSTYLE        = LIGHT
HTML_EXTRA_STYLESHEET  = ${SIDERA_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome.css ${SIDERA_REPO_ROOT_DIR}/docs/custom.css
HTML_EXTRA_FILES       = ${SIDERA_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-fragment-copy-button.js ${SIDERA_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-interactive-toc.js ${SIDERA_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-paragraph-link.js ${SIDERA_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-tabs.js ${SIDERA_REPO_ROOT_DIR}/docs/doxygen-awesome-css/doxygen-awesome-darkmode-toggle.js

HTML_HEADER            = ${SIDERA_REPO_ROOT_DIR}/docs/header.html

HTML_COLORSTYLE_HUE    = 209
HTML_COLORSTYLE_SAT    = 255
HTML_COLORSTYLE_GAMMA  = 113